{
  "name": "@microsoft/ocsdk",
  "version": "0.1.1-0",
  "description": "Microsoft Omnichannel SDK",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "karma start karma.conf.js",
    "start": "webpack-dev-server",
    "watch": "webpack --watch --config webpack.config.js --mode development",
    "build:tsc": "tsc --project tsconfig.json",
    "build:babel": "babel src --extensions .js,.ts --out-dir lib --verbose",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:prod": "npm run build:tsc && npm run build:babel && webpack --config webpack.config.js --mode production",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@dhigroup/karma-vsts-reporter": "^1.3.0",
    "@types/jasmine": "^3.5.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "prettier": "^1.18.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.5",
    "uglify-js": "^3.6.8",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "@types/node": "^12.12.7",
    "axios": "^0.19.0"
  }
}
